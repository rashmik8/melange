<h1>New timeline</h1>

<%= form_for :post do |f| %>
  <p>
    <%= f.label :heading %><br>
    <%= f.text_field :heading %>
  </p>
 
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.label :genre %><br>
    <%= f.text_area :genre %>
  </p>

  <p>
    <%= f.label :featured_type %><br>
    <%= f.text_area :featured_type %>
  </p>
 
  <p>
    <%= f.label :startDate %><br>
    <%= f.text_area :startDate %>
  </p>

  <p><%= f.submit "Create Timeline" %></p>
<% end %>

<%= link_to 'Back', timelines_path %>
